version: '2'
services:
    web:
      restart: always
      image: nginx
      container_name: {YOUR_PROJECT_NAME}
      volumes:
          - "./etc/nginx/default.conf:/etc/nginx/conf.d/default.conf"
          - "./src:/var/www/html"
      #ports:
      #    - "86:80"
      command: [nginx-debug, '-g', 'daemon off;']

networks:
  default:
      external:
          name: imdockgroup